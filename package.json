{
  "name": "@volkovasystem/result",
  "version": "0.0.0",
  "description": "Result class interface for procedure return value.",
  "main": "result.js",
  "scripts": {
    "test": "node ./result.test.js",
    "clean": "rm -Rfv ./node_modules && rm ./result.js",
    "compress": "npx terser ./result.module.js --compress --comments all --output ./result.js",
    "modularize": "npx babel --plugins @babel/plugin-transform-modules-umd ./result.js --out-file ./result.js",
    "minify": "npx terser ./result.js --compress --comments all --output ./result.js",
    "build": "npm run compress && npm run modularize && npm run minify",
    "deploy": "npm run clean && npm install && npm run build && npm test",
    "prepublishOnly": "npm run clean && npm install && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystem/result.git"
  },
  "keywords": [
    "result",
    "return"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystem/result/issues"
  },
  "homepage": "https://github.com/volkovasystem/result#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-umd": "^7.9.0"
  }
}
